    "page = requests.get(url)\n",
    "soup = BeautifulSoup(page.content,'html.parser')\n",
    "\n",
    "#Equipos\n",
    "\n",
    "equipo = soup.find_all('span', class_='nombre-equipo');\n",
    "\n",
    "print(equipo)\n"
